scopeName: 'text.tagml'
name: 'TAGML'
fileTypes: [ 'tagml', 'tag' ]
limitLineLength: false

patterns: [
  {
    comment: 'namespace'
    name: 'namespace.tagml'
    match: '\\[!ns [^]]+]'
  },
  {
    comment: 'comment'
    name: 'comment.tagml'
    match: '\\[!--.*--]'
  },
  {
    comment: 'set.open'
    name: 'set.open.tagml'
    match: '\\[!--.*--]'
  },

]